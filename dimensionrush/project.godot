config_version=5

[application]

config/name="DimensionRush"
run/main_scene="res://scenes/main.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://scripts/manager/game_manager.gd"
SignalManager="*res://scripts/manager/signal_manager.gd"
SoundManager="*res://scripts/manager/sound_manager.gd"

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="viewport"
window/stretch/scale=0.5
mouse_cursor/custom_image="res://assets/kenney_crosshairPack/PNG/Outline/crosshair037.png"
mouse_cursor/custom_image_hotspot=Vector2(36, 36)

[file_customization]

folder_colors={
"res://assets/": "red",
"res://builds/": "pink",
"res://resources/": "orange",
"res://scenes/": "yellow",
"res://scripts/": "green",
"res://shader/": "purple"
}

